{
  "expo": {
    "name": "睇戲2",
    "slug": "movie-tv-2",
    "version": "1.0.0",
    "orientation": "landscape",
    "icon": "./assets/splash.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#cbd5e1"
