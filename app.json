      "adaptiveIcon": {
        "foregroundImage": "./assets/splash.png",
        "backgroundColor": "#cbd5e1"
      },
      "intentFilters": [
        {
          "action": "MAIN",
          "data": {},
          "category": ["LEANBACK_LAUNCHER", "LAUNCHER"]
        }
      ],
      "package": "com.workmichaelma.movietv2"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
